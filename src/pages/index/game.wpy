<script>
  import wepy from 'wepy'
  import api from '../../interfaces/api'
  import wxApi from '../../interfaces/wxapi'
  import shake from '../../interfaces/shake'
  export default class SetSign extends wepy.page {
    config = {
      navigationBarTitleText: '摇一摇 赚积分'
    }

    data = {
      sys:null,
      right:'100%',
      transtion:'',
      result:'',
      myInfo:null,
      isShake:0,
    }

    methods = {
      async gameStart(){
        this.transtion = 'transition: 1s ease;'
        this.right = 0
        new shake(this,{
          shakeThreshold: 70, //阈值
          callback: (idx, award) => {
            console.log(arguments)
          }
        })
      }
    }
    async cj() {
      const data = await api.toCj()
      if(data.code==200){
        this.result = data
      }
    }
    async onLoad() {
      const data = await wxApi.getSystemInfo()
      const list = await api.cjList()
      this.sys = data
      this.list = list
      this.$apply()
    }
    async onShow() {
      const myInfo = await api.getPersonalInfo()
      this.myInfo = myInfo.data
      this.$apply()
    }
  }
</script>
