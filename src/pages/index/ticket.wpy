<template>
  <view class="container ticket">
      <view class="ticket_item bgwhite" wx:for="{{list}}" wx:key="ticket_{{index}}">
          <view wx:if="{{item.status=='已使用'}}" class="status used"></view>
          <view wx:if="{{item.status=='已过期'}}" class="status dated"></view>
          <view class="flexrow first_row">
              <view class="flexrow_bd"><text class="money">{{item.merchant_name}}</text></view>
              <view class="flexrow_bd qrcode_wrap">兑换码：{{item.code}}</view>
          </view>
          <view class="flexrow second_row">
              <view class="flexrow_bd"><text>消费满{{item.full}}减{{item.cut}}</text></view>
              <view class="flexrow_bd"><text>有效期：{{item.deadline}}</text></view>
          </view>
      </view>
  </view>
</template>

<script>
  import wepy from 'wepy'
  import api from '../../interfaces/api'
  import wxApi from '../../interfaces/wxapi'
  export default class Ticket extends wepy.page {
    config = {
      navigationBarTitleText: '优惠券'
    }
    components = {
    }


    data = {
      list:null
    }



    methods = {
      
    }

    events = {

    }

    async onShow() {
      const data = await api.getTicket()
      if(data.code==200){
          this.list = data.data
          this.$apply();
      }
    }
  }
</script>
