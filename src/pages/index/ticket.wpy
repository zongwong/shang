            .flexrow{
                display: flex;
                align-items: center;
                justify-content: space-between;
                padding:14rpx 32rpx;
                .qrcode_wrap{
                     
                }
                .qrcode{
                    width: 330rpx;
                    height: 68rpx;
                }
            }
            .first_row{
                border-bottom: 2rpx dashed #e9e9e9;
            }
            .second_row{
                font-size: 24rpx;
                color: #a6a6a6;
            }
            &:before,&:after{
                content: '';
                width: 20rpx;
                height: 20rpx;
                background: #f1f1f1;
                border-radius: 50%;
                position: absolute;
                top: 100rpx;
            }
            &:before{
                left: -10rpx;
            }
            &:after{
                right: -10rpx;
