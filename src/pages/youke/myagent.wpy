<style lang="less">
.agent{
  font-size: 28rpx;
  .weui-cell{
    padding: 24rpx;
  }
  .icon_adr{
    width: 16rpx;
    height: 21rpx;
    background-image: url(data:image/png;base64,);
    vertical-align: middle;
    margin: 0 6rpx 0 15rpx;
  }
  .icon_phone{
    width: 56rpx;
    height: 56rpx;
    background-image: url(data:image/png;base64,);
    // background-size: cover;
  }
  .info_box{
    width: 645rpx;
    display: flex;
    align-items: center;
    .info_right{
      flex:1;
      max-width: 522rpx;
    }
    .avt{
      display: block;
      border-radius: 50%;
      width: 90rpx;
      height: 90rpx;
      margin-right: 20rpx;
    }
    .area{
      color: #606060;
      font-size: 26rpx;
    }
    .adr_detail{
      color: #959595;
      font-size: 24rpx;
    }
  }
}
</style>
<template>
  <view class="container agent">
    <view class="weui-cells">
      <view class="weui-cell" wx:for="{{list}}" wx:key="agent">
        <view class="weui-cell__bd">
          <view class="info_box">
            <image src="{{item.headimgurl||'../../images/avatar.png'}}" class="avt"></image>
            <view class="info_right">
              <view>{{item.agent_name}} <text class="icon icon_adr"></text><text class="area">{{item.city}}</text></view>
              <view class="adr_detail">地址：{{item.address}}</view>
            </view>
          </view>
        </view>
        <view class="weui-cell__ft icon icon_phone" @tap="phoneCall({{item.mobilephone}})"></view>
      </view>
    </view>
    
  </view>
</template>

<script>
  import wepy from 'wepy'
  import api from '../../interfaces/api'
  import wxApi from '../../interfaces/wxapi'
  export default class MyAgent extends wepy.page {
    config = {
      navigationBarTitleText: '我的代理'
    }



    data = {
      list:null

    }
    methods= {
      phoneCall(phone){
        wxApi.phoneCall(phone)
      }
    }
    async onLoad() {
        this.top()
    }
    async top(){
        const data = await api.myAgent()
        if(data.code==200){
            this.list = data.data
            this.$apply()
        }
    }

  }
</script>
